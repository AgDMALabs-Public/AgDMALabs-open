{
    "$defs": {
        "ApplicationEvent": {
            "additionalProperties": false,
            "description": "Represents a single application event (e.g., fertilizer, pesticide, herbicide).",
            "example": {
                "applicationRate": 2.0,
                "applicationType": "Herbicide",
                "equipment": "Tractor Sprayer",
                "eventId": "1234-679-123-456-4456",
                "location": {
                    "crs": "EPSG:4326",
                    "elevation_m": 150.5,
                    "geometry": "POINT (-118.2437 34.0522)",
                    "id": "loc-uuid-12345",
                    "name": "Research Plot A"
                },
                "method": "Broadcast",
                "mixName": "Glyphosate 41%",
                "notes": "Post-emergent herbicide application on weeds.",
                "rateUnit": "L/ha",
                "schema_name": "ApplicationEvent",
                "timestamp": "2025-05-15T10:30:00Z"
            },
            "properties": {
                "schema_name": {
                    "const": "ApplicationEvent",
                    "description": "The name of the schema for this model.",
                    "title": "Schema Name",
                    "type": "string"
                },
                "eventId": {
                    "description": "Unique identifier for this specific application event.",
                    "examples": [
                        "APP-2025-FIELD-A-002"
                    ],
                    "title": "Eventid",
                    "type": "string"
                },
                "location": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/Location"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null
                },
                "timestamp": {
                    "description": "The date and time the application occurred.",
                    "format": "date-time",
                    "title": "Timestamp",
                    "type": "string"
                },
                "applicationType": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The type of application (e.g., 'Fertilizer', 'Herbicide', 'Fungicide', 'Insecticide').",
                    "examples": [
                        "Herbicide"
                    ],
                    "title": "Applicationtype"
                },
                "mixName": {
                    "description": "The name of the product applied.",
                    "examples": [
                        "Roundup PowerMAX"
                    ],
                    "title": "Mixname",
                    "type": "string"
                },
                "applicationRate": {
                    "description": "The rate at which the product was applied.",
                    "examples": [
                        2.5
                    ],
                    "minimum": 0,
                    "title": "Applicationrate",
                    "type": "number"
                },
                "rateUnit": {
                    "description": "The unit for the application rate (e.g., 'L/ha', 'gal/acre', 'kg/ha').",
                    "examples": [
                        "L/ha"
                    ],
                    "title": "Rateunit",
                    "type": "string"
                },
                "method": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The method of application (e.g., 'Broadcast', 'Foliar', 'In-furrow', 'Side-dress').",
                    "examples": [
                        "Foliar"
                    ],
                    "title": "Method"
                },
                "equipment": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The equipment used for the application.",
                    "examples": [
                        "Sprayer"
                    ],
                    "title": "Equipment"
                },
                "notes": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Any additional notes about the application event.",
                    "title": "Notes"
                }
            },
            "required": [
                "schema_name",
                "eventId",
                "timestamp",
                "mixName",
                "applicationRate",
                "rateUnit"
            ],
            "title": "ApplicationEvent",
            "type": "object"
        },
        "HarvestEvent": {
            "additionalProperties": false,
            "description": "Represents a single tillage event for a field.",
            "example": {
                "cropYield": 250.75,
                "cropYieldUnits": "bu/acre",
                "eventId": "HARV-2025-FIELD-A-001",
                "harvestType": "Destructive",
                "implementUsed": "Combine Harvester",
                "location": {
                    "crs": "EPSG:4326",
                    "elevation_m": 120.0,
                    "geometry": "POINT (-118.456 34.123)",
                    "id": "loc-harvest-789",
                    "latitude": 34.123,
                    "longitude": -118.456,
                    "name": "Field A Harvest Zone 1"
                },
                "notes": "First pass of corn harvest in Field A. Good yield observed.",
                "timestamp": "2025-09-20T10:00:00Z"
            },
            "properties": {
                "eventId": {
                    "description": "Unique identifier for this specific harvest event.",
                    "examples": [
                        "TILL-2025-FIELD-A-003"
                    ],
                    "title": "Eventid",
                    "type": "string"
                },
                "location": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/Location"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null
                },
                "timestamp": {
                    "description": "The date and time the harvest occurred.",
                    "format": "date-time",
                    "title": "Timestamp",
                    "type": "string"
                },
                "harvestType": {
                    "description": "The type of harvest performed (Destructive, selective).",
                    "examples": [
                        "Destructive"
                    ],
                    "title": "Harvesttype",
                    "type": "string"
                },
                "implementUsed": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The specific implement used for harvest.",
                    "examples": [
                        "Hand"
                    ],
                    "title": "Implementused"
                },
                "cropYield": {
                    "anyOf": [
                        {
                            "minimum": 0,
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "the average crop harvest for the harvest event",
                    "examples": [
                        20.0
                    ],
                    "title": "Cropyield"
                },
                "cropYieldUnits": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The units for the crop harvest.",
                    "examples": [
                        "bu/acre"
                    ],
                    "title": "Cropyieldunits"
                },
                "notes": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Any additional notes about the harvest event.",
                    "title": "Notes"
                }
            },
            "required": [
                "eventId",
                "timestamp",
                "harvestType"
            ],
            "title": "HarvestEvent",
            "type": "object"
        },
        "Location": {
            "additionalProperties": false,
            "description": "Values to support the proper documentation of location information",
            "example": {
                "admin_level_0": "USA",
                "admin_level_1": "California",
                "admin_level_2": "Los Angeles County",
                "admin_level_3": "Los Angeles",
                "crs": "EPSG:4326",
                "elevation_m": 150.5,
                "field": "Field_7B",
                "geometry": "POINT (-118.2437 34.0522)",
                "id": "loc-uuid-12345",
                "latitude": 34.0522,
                "location": "A",
                "longitude": -118.2437,
                "name": "Research Plot A",
                "site": "AgTech Research Farm"
            },
            "properties": {
                "id": {
                    "description": "The UUID of the location",
                    "title": "Id",
                    "type": "string"
                },
                "name": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The name of the location",
                    "title": "Name"
                },
                "latitude": {
                    "anyOf": [
                        {
                            "maximum": 90.0,
                            "minimum": -90.0,
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The latitude of where the data was taken",
                    "title": "Latitude"
                },
                "longitude": {
                    "anyOf": [
                        {
                            "maximum": 180.0,
                            "minimum": -180.0,
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The longitude of where the data was taken",
                    "title": "Longitude"
                },
                "elevation_m": {
                    "anyOf": [
                        {
                            "maximum": 10000,
                            "minimum": -100,
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The elevation of where the data came from",
                    "title": "Elevation M"
                },
                "crs": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The crs of the GPS data",
                    "title": "Crs"
                },
                "geometry": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The Geometry in WKT format",
                    "title": "Geometry"
                },
                "admin_level_0": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "This level is for Country / Nation.",
                    "title": "Admin Level 0"
                },
                "admin_level_1": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "This is for things like state or the primary subdivision of a country / Nation.",
                    "title": "Admin Level 1"
                },
                "admin_level_2": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "This is for things like county or the primary subdivision of a state.",
                    "title": "Admin Level 2"
                },
                "admin_level_3": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "This is for things like city/town/village or the primary subdivision of a county.",
                    "title": "Admin Level 3"
                },
                "site": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The site refers to the research site the data came from. A site will have many fields and locations associated to it",
                    "title": "Site"
                },
                "field": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "What is the field ID that the data came from",
                    "title": "Field"
                },
                "location": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "the location (by BrApi Def) of there the data came from",
                    "title": "Location"
                }
            },
            "required": [
                "id"
            ],
            "title": "Location",
            "type": "object"
        },
        "PlantingEvent": {
            "additionalProperties": false,
            "description": "Represents a single planting event for a field.",
            "example": {
                "cropType": "Corn",
                "depthCm": 5.5,
                "eventId": "PLANT-2025-FIELD-A-001",
                "location": {
                    "crs": "EPSG:4326",
                    "elevation_m": 160.0,
                    "geometry": "POINT (-118.789 34.567)",
                    "id": "loc-plant-abc",
                    "latitude": 34.567,
                    "longitude": -118.789,
                    "name": "Field A Section 1"
                },
                "notes": "Field A planted with corn, good soil moisture.",
                "seedingRate": 34500,
                "seedingUnit": "seeds/acre",
                "timestamp": "2025-04-20T08:00:00Z",
                "variety": "Pioneer P1197AM"
            },
            "properties": {
                "eventId": {
                    "description": "Unique identifier for this specific planting event.",
                    "examples": [
                        "PLANT-2025-FIELD-A-001"
                    ],
                    "title": "Eventid",
                    "type": "string"
                },
                "location": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/Location"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The location of the planting event (e.g., 'Field A')."
                },
                "timestamp": {
                    "description": "The date and time the planting occurred.",
                    "format": "date-time",
                    "title": "Timestamp",
                    "type": "string"
                },
                "cropType": {
                    "description": "The type of crop planted (e.g., 'Corn', 'Soybeans', 'Wheat').",
                    "examples": [
                        "Corn"
                    ],
                    "title": "Croptype",
                    "type": "string"
                },
                "variety": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Specific variety or hybrid of the crop.",
                    "examples": [
                        "Dekalb DKC67-44RIB"
                    ],
                    "title": "Variety"
                },
                "seedingRate": {
                    "description": "The rate at which seeds were planted.",
                    "examples": [
                        34000
                    ],
                    "title": "Seedingrate",
                    "type": "number"
                },
                "seedingUnit": {
                    "description": "The unit for the seeding rate (e.g., 'seeds/acre', 'seeds/ha').",
                    "examples": [
                        "seeds/acre"
                    ],
                    "title": "Seedingunit",
                    "type": "string"
                },
                "depthCm": {
                    "anyOf": [
                        {
                            "minimum": 0,
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Planting depth in centimeters.",
                    "examples": [
                        5.0
                    ],
                    "title": "Depthcm"
                },
                "notes": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Any additional notes about the planting event.",
                    "title": "Notes"
                }
            },
            "required": [
                "eventId",
                "timestamp",
                "cropType",
                "seedingRate",
                "seedingUnit"
            ],
            "title": "PlantingEvent",
            "type": "object"
        },
        "TillageEvent": {
            "additionalProperties": false,
            "description": "Represents a single tillage event for a field.",
            "example": {
                "depthCm": 25.0,
                "eventId": "TILL-2025-FIELD-A-003",
                "implementUsed": "Case IH Chisel Plow",
                "notes": "Primary tillage after winter. Good soil penetration.",
                "tillageType": "Chisel Plow",
                "timestamp": "2025-03-10T14:00:00Z"
            },
            "properties": {
                "eventId": {
                    "description": "Unique identifier for this specific tillage event.",
                    "examples": [
                        "TILL-2025-FIELD-A-003"
                    ],
                    "title": "Eventid",
                    "type": "string"
                },
                "timestamp": {
                    "description": "The date and time the tillage occurred.",
                    "format": "date-time",
                    "title": "Timestamp",
                    "type": "string"
                },
                "tillageType": {
                    "description": "The type of tillage performed (e.g., 'Conventional', 'No-till', 'Minimum-till', 'Chisel Plow').",
                    "examples": [
                        "Chisel Plow"
                    ],
                    "title": "Tillagetype",
                    "type": "string"
                },
                "implementUsed": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The specific implement used for tillage.",
                    "examples": [
                        "John Deere 2700 Ripper"
                    ],
                    "title": "Implementused"
                },
                "depthCm": {
                    "anyOf": [
                        {
                            "minimum": 0,
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Average tillage depth in centimeters.",
                    "examples": [
                        20.0
                    ],
                    "title": "Depthcm"
                },
                "notes": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Any additional notes about the tillage event.",
                    "title": "Notes"
                }
            },
            "required": [
                "eventId",
                "timestamp",
                "tillageType"
            ],
            "title": "TillageEvent",
            "type": "object"
        }
    },
    "additionalProperties": false,
    "description": "Represents the management history for a single agricultural field over a season,\nincluding planting, application, and tillage events.",
    "example": {
        "application_events": [
            {
                "applicationRate": 2.0,
                "applicationType": "Herbicide",
                "equipment": "Tractor Sprayer",
                "eventId": "APP-2025-FIELD-A-002",
                "method": "Broadcast",
                "notes": "Post-emergent herbicide application on weeds.",
                "productName": "Glyphosate 41%",
                "rateUnit": "L/ha",
                "timestamp": "2025-05-15T10:30:00Z"
            },
            {
                "applicationRate": 150.0,
                "applicationType": "Fertilizer",
                "equipment": "Fertilizer Spreader",
                "eventId": "APP-2025-FIELD-A-004",
                "method": "Side-dress",
                "notes": "Nitrogen application at V8 growth stage.",
                "productName": "Urea",
                "rateUnit": "kg/ha",
                "timestamp": "2025-07-01T09:00:00Z"
            }
        ],
        "fieldId": "FIELD-A-2025",
        "harvest_events": [
            {
                "crop_yield": 200,
                "crop_yield_units": "bu/acre",
                "eventId": "TILL-2025-FIELD-A-003",
                "harvestType": "destructive",
                "harvest_method": "machine",
                "notes": "The first and only harvest of the season.",
                "timestamp": "2025-03-10T14:00:00Z"
            }
        ],
        "planting_events": [
            {
                "cropType": "Corn",
                "depthCm": 5.5,
                "eventId": "PLANT-2025-FIELD-A-001",
                "notes": "Field A planted with corn, good soil moisture.",
                "seedingRate": 34500,
                "seedingUnit": "seeds/acre",
                "timestamp": "2025-04-20T08:00:00Z",
                "variety": "Pioneer P1197AM"
            }
        ],
        "seasonYear": 2025,
        "tillage_events": [
            {
                "depthCm": 25.0,
                "eventId": "TILL-2025-FIELD-A-003",
                "implementUsed": "Case IH Chisel Plow",
                "notes": "Primary tillage after winter. Good soil penetration.",
                "tillageType": "Chisel Plow",
                "timestamp": "2025-03-10T14:00:00Z"
            }
        ]
    },
    "properties": {
        "fieldId": {
            "description": "Unique identifier for the agricultural field.",
            "examples": [
                "FIELD-A-2025"
            ],
            "title": "Fieldid",
            "type": "string"
        },
        "seasonYear": {
            "description": "The calendar year for which this management data is relevant.",
            "examples": [
                2025
            ],
            "title": "Seasonyear",
            "type": "integer"
        },
        "planting_events": {
            "description": "A list of all the planting events.",
            "items": {
                "$ref": "#/$defs/PlantingEvent"
            },
            "title": "Planting Events",
            "type": "array"
        },
        "application_events": {
            "description": "A list of all the application events.",
            "items": {
                "$ref": "#/$defs/ApplicationEvent"
            },
            "title": "Application Events",
            "type": "array"
        },
        "tillage_events": {
            "description": "A list of all the tillage events.",
            "items": {
                "$ref": "#/$defs/TillageEvent"
            },
            "title": "Tillage Events",
            "type": "array"
        },
        "harvest_events": {
            "description": "A list of all the harvest Events.",
            "items": {
                "$ref": "#/$defs/HarvestEvent"
            },
            "title": "Harvest Events",
            "type": "array"
        }
    },
    "required": [
        "fieldId",
        "seasonYear",
        "planting_events",
        "application_events",
        "tillage_events",
        "harvest_events"
    ],
    "title": "FieldManagement",
    "type": "object"
}