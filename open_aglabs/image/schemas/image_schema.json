                "object_resolution": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Image Level / Collection Level depending on variance.",
                    "title": "Object Resolution"
                },
                "object_resolution_ml": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/MLOutput"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The resolution of the object in the image predicted by a ML model."
                },
                "light_source": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "What light source was used to collect the image.",
                    "title": "Light Source"
                },
                "lighting_lux": {
                    "anyOf": [
                        {
                            "maximum": 100000,
                            "minimum": 0,
                            "type": "number"
