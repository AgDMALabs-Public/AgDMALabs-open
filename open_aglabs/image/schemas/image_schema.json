                    "default": null,
                    "description": "The time the image was taken",
                    "title": "Time"
                },
                "camera_height_m": {
                    "anyOf": [
                        {
                            "minimum": 0,
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The height of the camera in meters",
                    "title": "Camera Height M"
                },
                "camera_angle_deg": {
                    "anyOf": [
                        {
                            "maximum": 180,
                            "minimum": -180,
                            "type": "number"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
